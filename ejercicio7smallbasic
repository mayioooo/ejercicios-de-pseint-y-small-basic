TextWindow.WriteLine("Calculadora de promedio")
TextWindow.WriteLine("-------------------------")
TextWindow.WriteLine("Ingrese el primer número:")
num1 = TextWindow.ReadNumber()
TextWindow.WriteLine("Ingrese el segundo número:")
num2 = TextWindow.ReadNumber()
TextWindow.WriteLine("Ingrese el tercer número:")
num3 = TextWindow.ReadNumber()

promedio = (num1 + num2 + num3) / 3

TextWindow.WriteLine("El promedio es: " + promedio)
